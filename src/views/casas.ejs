<!DOCTYPE html>
<html>
<head>
 <meta charset="UTF-8">

 <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
 
</head>
<body>

    <div class="container" style="margin-left: 2%;">
        <div class="row">
            <div class="col-md-7">
                <table class="table table-bordered table-hover">
                  <thead>
                    <tr>
                        <th>_id</th>
                        <th>Name</th>
                        <th>Description</th>
                        <th>Bed_type</th>
                        <th>price</th>
                        <th>id_customers</th>
                    </tr>

                  </thead>
                  <tbody>
                   <% for(var i = 0 ; i<Rentas.length ; i++){%>
                  <tr>
                    <td> <%= i+ 1 %></td>
                    <td> <%= Rentas[i]._id %></td>
                    <td> <%= Rentas[i].Name %></td>
                    <td> <%= Rentas[i].Description %></td>
                    <td> <%= Rentas[i].Bed_type %></td>
                    <td> <%= Rentas[i].price %></td>
                    <td> <%= Rentas[i].id_customers %></td>
                </tr>
                    <% } %>
                  </tbody>

                </table>

            </div>
            </div>
        </div>
    </div>
    
   
</body>
</html>